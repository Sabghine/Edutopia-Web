{% extends 'base.html.twig' %}

{% block title %}WorkDone{% endblock %}

{% block body %}
    <h1>WorkDone</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ work_done.id }}</td>
            </tr>
            <tr>
                <th>WorkFile</th>
                <td>{{ work_done.workFile }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ work_done.status }}</td>
            </tr>
            <tr>
                <th>Score</th>
                <td>{{ work_done.score }}</td>
            </tr>
            <tr>
