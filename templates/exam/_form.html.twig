<table class="table table-striped">
    {{ form_start(form) }}
    <tr>
        <td>{{ form_label(form.type)  }}</td>
        <td>{{ form_widget(form.type,{ 'attr': {'class': 'form-control '}})  }}</td>
    </tr>

    <tr>
        <td>{{ form_label(form.startDate)  }}</td>
        <td>{{ form_widget(form.startDate,{ 'attr': {'class': 'form-control'}})  }}</td>
    </tr>

    <tr>
        <td>{{ form_label(form.finishDate)  }}</td>
        <td>{{ form_widget(form.finishDate,{ 'attr': {'class': 'form-control'}})  }}</td>
    </tr>

    <tr>
        <td>{{ form_label(form.createdBy)  }}</td>
        <td>{{ form_widget(form.createdBy,{ 'attr': {'class': 'form-control'}})  }}</td>
    </tr>


    <tr>

        <td> <button class="btn btn-success" id="btt">{{ button_label|default('Save') }}</button></td>
        <td></td>
    </tr>
    {{ form_end(form)  }}
</table>
